<div
  id="map"
  data-map-target="map"
  data-map-api-value= "<%= Rails.application.credentials.google_maps[:api_key] %>"
  data-map-places="<%= @places.to_json(include: :images)  %>"
  data-controller="map"
></div>

<div id="last-added-places-container" class="bg-beige">
<h3 class="large-font bg-brown beige">Son Eklenen Yerler</h3>

<div id="last-places-list">
  <% Place.last_ten.each do |place| %>
        <%= link_to "+#{place.name}", place_path(place), class: "place-card dark-green" %>
    <% end %>
</div>
</div>



